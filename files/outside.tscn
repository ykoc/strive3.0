	get_node(\"weightmeter/Label\").set_text(\"Weight: \" + str(weight.currentweight) + '/' + str(weight.maxweight))
	get_node(\"weightmeter\").set_value((weight.currentweight*10/max(weight.maxweight,1)*10))


