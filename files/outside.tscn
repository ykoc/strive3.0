func itemsshop():
	var shop = merchantitems
	for i in $ScrollContainer2/GridContainer.get_children():
		if i.get_name() != \"Button\":
			i.visible = false
			i.queue_free()
	
	for i in shop:
		var item = globals.itemdict[i]
		item = globals.itemdict[i]
