func itemsinventory():
	
	var button
	var array = []
	var tempitem
	
	for i in globals.itemdict.values():
		if i.amount < 1 || i.type in ['gear','dummy'] || (filter != '' && (i.name.findn(filter) < 0 && i.description.findn(filter) < 0)):
			continue
